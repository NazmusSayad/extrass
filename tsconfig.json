{
	"compilerOptions": {
		"target": "es6",
		"allowJs": true,
		"declaration": true,
		"pretty": true,
		"skipLibCheck": true,
		"strict": true,
		"moduleResolution": "node",
		"esModuleInterop": true,
		"noImplicitAny": false,
		"inlineSourceMap": false,
		"removeComments": true
	},
	"compileOnSave": false,
	"include": [
		"src"
	]
}